{
    "rules": {
        "comma-dangle": [1, "never"],
        "no-console": 2,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 2,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": [1, "all"],
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": [1, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "eqeqeq": 1,
        "no-alert": 2,
        "no-extra-bind": 1,
        "no-fallthrough": 2,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-octal": 2,
        "no-throw-literal": 1,
        "no-delete-var": 2,
        "no-label-var": 1,
        "no-shadow-restricted-names": 1,
        "no-undef": 0,
        "no-unused-vars": 2,
        "eol-last": 0,
        "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
        "no-multiple-empty-lines": [1, {
            "max": 2
        }],
        "quotes": [1, "single", "avoid-escape"],
        "arrow-parens": [1, "as-needed"],
        "arrow-spacing": [1, {
            "before": true,
            "after": true
        }],
        "constructor-super": 1,
        "no-class-assign": 1,
        "no-const-assign": 1,
        "no-this-before-super": 1,
        "no-var": 1,
        "object-shorthand": [1, "always"],
        "prefer-const": 1,
        "prefer-spread": 1,
        "require-yield": 1,
        "no-bitwise": 1
    },
    "parserOptions": {
        "ecmaVersion": 6
    }
}